# Zerodha Kite Connect API Configuration
# Copy this file to .env and fill in your actual values

# Your Zerodha API credentials
API_KEY=your_kite_api_key_here
API_SECRET=your_kite_api_secret_here

# Optional: Default trading parameters
DEFAULT_QTY=1
DEFAULT_SYMBOL=BANKNIFTY

# Optional: Trading session settings
MARKET_START_TIME=09:15
MARKET_END_TIME=15:30
SQUARE_OFF_TIME=15:10

# Optional: Risk management
DAILY_STOP_LOSS=200
MAX_TRADES_PER_DAY=10

# Optional: Enable live trading (set to 1 to enable real orders)
ENABLE_LIVE_ORDERS=0
